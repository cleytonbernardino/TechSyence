{
  "ConnectionStrings": {
    "Connection": "NONE"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day",
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {SourceContext} {Message:lj}{NewLine}{Exception}"
        }
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "Settings": {
    "IdCryptographyAlphabet": "aJVu3P4s0foxAqivmTdrGO1ynS6eMtRLwEFzZkDgCNcj2IHpK7l5bXYWhUBQ89",
    "JWT": {
      "ExpiryInMinutes": 1000,
      "SigningKey": "05F37Y6zj7HO57mEFXIalP1nj0EHiLs4",
      "Issuer": "https://localhost:7055"
    }
  }
}